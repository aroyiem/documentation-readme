{
  "name": "documentation-readme",
  "version": "1.0.0",
  "description": "inject documentation into your README.md",
  "main": "lib/plugin.js",
  "bin": {
    "documentation-readme": "bin/documentation-readme"
  },
  "scripts": {
    "lint": "eslint --no-eslintrc -c .eslintrc .",
    "test": "npm run lint && tap --coverage test/*.js",
    "docs": "node index.js -s \"mdast plugin\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/documentationjs/documentation-readme.git"
  },
  "keywords": [
    "documentation",
    "jsdoc",
    "readme"
  ],
  "author": "Anand Thakker <vestibule@anandthakker.net> (http://anandthakker.net/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/documentationjs/documentation-readme/issues"
  },
  "homepage": "https://github.com/documentationjs/documentation-readme#readme",
  "devDependencies": {
    "eslint": "^1.5.1",
    "tap": "^2.0.0"
  },
  "dependencies": {
    "documentation": "^2.0.1",
    "mdast": "^2.0.0",
    "yargs": "^3.26.0"
  }
}
